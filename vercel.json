{
  "version": 2,
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_URL": "https://kmci-website.vercel.app",
    "LOG_LEVEL": "info"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm ci",
  "devCommand": "npm run dev"
}
